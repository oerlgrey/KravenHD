#: KravenHD.py
msgid "remaining: "
msgstr "zostáva:"

#: KravenHD.py
msgid "Busy color"
msgstr "Obsadený tuner - Farba"

#: KravenHD.py
msgid "Choose the color for engaged tuners. Press OK to define your own RGB color."
msgstr "Vyberte farbu pre obsadené (zaneprázdnené) tunery. Stlačením tlačidla OK definujte svoju vlastnú farbu RGB."

#: KravenHD.py
msgid "Active color"
msgstr "Aktívny tuner - Farba"

#: KravenHD.py
msgid "Choose the color for the current live tuner. Press OK to define your own RGB color."
msgstr "Vyberte farbu aktuálne použitého tunera. Stlačením tlačidla OK definujte svoju vlastnú farbu RGB."

#: KravenHD.py
msgid "Record color"
msgstr "Tuner nahráva - Farba"

#: KravenHD.py
msgid "Choose the color for recording tuners. Press OK to define your own RGB color."
msgstr "Vyberte farbu pre tunery s ktorých sa prave nahráva. Stlačením tlačidla OK definujte svoju vlastnú farbu RGB."

#: KravenHD.py
msgid "Active record color"
msgstr "Aktívny - Nahráva - Farba"

#: KravenHD.py
msgid "Choose the color for the current live tuner when also recording. Press OK to define your own RGB color."
msgstr "Vyberte farbu aktuálne použitého tunera, ktorý prave nahráva. Stlačením tlačidla OK definujte svoju vlastnú farbu RGB.."

#: KravenHD.py
msgid "Show unused Tuners"
msgstr "Zobraziť nepoužívané tunery"

#: KravenHD.py
msgid "Choose whether unused tuners are displayed or not."
msgstr "Vyberte, či sa nepoužívané tunery majú alebo nemajú zobrazovať v infobare."

#: KravenHD.py
msgid "Show AGC/SNR"
msgstr "Zobraziť AGC/SNR"

#: KravenHD.py
msgid "Choose whether AGC/SNR are displayed or not."
msgstr "Vyberte, či sa AGC/SNR majú alebo nemajú zobrazovať v infobare."

#: KravenHD.py
msgid "Skin Resolution"
msgstr "Rozlíšenie Skinu"

#: KravenHD.py
msgid "Choose the resolution of the skin."
msgstr "Zvoľte rozlíšenie pre Skin."

